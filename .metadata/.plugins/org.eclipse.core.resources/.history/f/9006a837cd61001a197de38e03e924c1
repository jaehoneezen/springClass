package spring.DI;

public class Foo {
	String str;
	int i;
	
	private Bar bar;
	private Baz baz;
	
	public Foo(String str, int i, Bar bar) {
		this.bar = bar;
		this.str = str;
		this.i = i;
	}
	
	public Foo(Bar bar, Baz baz) {
		this.bar = bar;
		this.baz = baz;
	}
	
	public void doFoo() {
		System.out.println("doFoo() 실행");
		bar.doBar();
		baz.doBaz();
	}
	
	public void doFoo2() {
		System.out.println(str + " " + i + " ");
		bar.doBar();
	}

	
}
